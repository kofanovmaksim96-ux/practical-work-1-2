#include <iostream>
#include <string>
using namespace std;

int main() {
    setlocale(LC_ALL, "Rus");
    string password;
    string correct_password = "12345";

    cout << "Введите пароль: ";
    cin >> password;


    while (password != correct_password) {
        cout << "[ - ] Неверный пароль!" << endl;
        cout << "Введите пароль: ";
        cin >> password;
    }

 
    cout << "[ + ] Добро пожаловать!" << endl;

    return 0;
}
