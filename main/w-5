#include <iostream>
using namespace std;

int main() {
    setlocale(LC_ALL, "Rus");
    int number;
    int i = 1;  

    cout << "Введите число: ";
    cin >> number;

    cout << endl;

    while (i <= 10) {
        cout << "[ + ] " << number << " × " << i << " = " << number * i << endl;
        i = i + 1;
    }

    return 0;
}
